{
  "breadcrumbs.enabled": true,
  "breadcrumbs.symbolPath": "off",
  "cSpell.language": "en-GB",
  "cSpell.userWords": [
    "adminable",
    "builtin",
    "bulleted",
    "checkbox",
    "colors",
    "dialog",
    "draggables",
    "dropzone",
    "easymotion",
    "fallback",
    "hacky",
    "iosevka",
    "kamil",
    "lexer",
    "menlo",
    "monospace",
    "placeholder",
    "popd",
    "popover",
    "pushd",
    "refetch",
    "resaveable",
    "saml",
    "sienkiewicz",
    "sizer",
    "symlinks",
    "tmux",
    "toggleable",
    "tunnelblick",
    "unmount",
    "virtualenv"
  ],
  "diffEditor.renderSideBySide": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.autoClosingBrackets": "never",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeLens": false,
  "editor.cursorSurroundingLines": 5,
  "editor.fontFamily": "JetBrainsMono Nerd Font, Input Mono, Iosevka Nerd Font, Iosevka, Menlo, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 11,
  "editor.fontWeight": 500,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.letterSpacing": 0,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    80,
    100,
    120
  ],
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "top",
  "editor.stickyScroll.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "explorer.compactFolders": false,
  "explorer.fileNesting.enabled": true,
  "explorer.sortOrder": "filesFirst",
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 5000,
  "files.exclude": {
    "**/*.pyc": true
  },
  "git.blame.editorDecoration.enabled": true,
  "git.mergeEditor": true,
  "keyboard.touchbar.enabled": false,
  "markdown.preview.fontSize": 12,
  "markdown.preview.lineHeight": 1.2,
  "outline.collapseItems": "alwaysCollapse",
  "search.collapseResults": "alwaysCollapse",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font, Input Mono, Iosevka Nerd Font, Iosevka, Menlo, monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "normal",
  "vim.camelCaseMotion.enable": true,
  "vim.easymotion": true,
  "vim.easymotionMarkerBackgroundColor": "#222",
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.easymotionMarkerForegroundColorOneChar": "#f0f",
  "vim.textwidth": 100,
  "vim.useSystemClipboard": true,
  "window.commandCenter": true,
  "workbench.colorTheme": "Dracula Theme",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 8,
  "workbench.editor.pinnedTabSizing": "shrink",
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.iconTheme": "vscode-icons",
  "workbench.list.horizontalScrolling": true,
  "workbench.preferredDarkColorTheme": "Dracula Theme",
  "workbench.preferredLightColorTheme": "Visual Studio Light",
  "workbench.tree.indent": 10
}